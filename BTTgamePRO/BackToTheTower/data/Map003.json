"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"chara_shanzi0","direction":2,"pattern":1,"characterIndex":0},"list":[{"code":213,"indent":0,"parameters":[0,1,true]},{"code":123,"indent":0,"parameters":["B",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"B","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"chara_shanzi0","direction":6,"pattern":1,"characterIndex":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["啊，梨梨……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["我……睡着了？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["这里是……啊，对，是教室……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["（刚才，梦里隐约见到的身影，是谁……）"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["善子……？你还好吗？"]},{"code":401,"indent":0,"parameters":["是不是身体不舒服？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["我……我没事，只是刚才做了奇怪的梦，睡糊涂了。"]},{"code":401,"indent":0,"parameters":["现在已经完全清醒了，别担心，梨梨。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["刚才和花丸她们练完舞蹈，想着先回教室收拾一下东西，再来钢琴"]},{"code":401,"indent":0,"parameters":["教室找你的，不知道怎么就睡着了，对不起。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["在教室睡觉可是会着凉的哦……"]},{"code":401,"indent":0,"parameters":["是不是因为马上要参加LOVE LIVE比赛，压力太大了才会这样的？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["嗯……也许吧。"]},{"code":401,"indent":0,"parameters":["话说回来，梨梨，比赛的歌曲创作还顺利吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["主歌倒是没什么问题……"]},{"code":401,"indent":0,"parameters":["但是副歌……总觉得还是有点不协调……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["诶……是副歌的部分还没定好吗？"]},{"code":401,"indent":0,"parameters":["我听说，作曲的人，都会有一段旋律每时每刻地萦绕在脑子里，甩"]},{"code":401,"indent":0,"parameters":["也甩不掉，只能写下来把它变成曲子，再去洗脑其他人。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["于是，这段时时刻刻缠绕着人的旋律，就成为了“副歌”！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["唔…为什么听了你的形容，感觉副歌变成了一种有毒的东西呢……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["对啊对啊，就是要让这种毒性渗透到听众的每一个细胞中，他们才"]},{"code":401,"indent":0,"parameters":["会记住歌曲，被歌曲所感染，对吧！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["想当年我在天界流浪的时候啊，曾经路过一座古老的神社，潮汐之"]},{"code":401,"indent":0,"parameters":["夜那里面就会隐隐传来一种诡异的歌声呢，听过一遍就完全中毒了！"]},{"code":401,"indent":0,"parameters":["这大概就是成功的副歌的典范吧！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["唔姆唔姆，“成功的副歌”……吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["嗯，后来我向附近村庄的人们打听了那首歌，虽然村民们也不清楚"]},{"code":401,"indent":0,"parameters":["那首歌的创作者和演唱者，但是姑且给它取了一个歌名，叫做《具"]},{"code":401,"indent":0,"parameters":["足虫之歌》。真是个浪漫的名字呀……对了，梨梨，不如我给你唱"]},{"code":401,"indent":0,"parameters":["两句吧，说不定能对你的作品有启发哦——"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["“大王具足虫~~~大王具足虫~~~”"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["诶诶，等、等一下！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["“大王大王大王大王具足虫~~~~~~”"]},{"code":401,"indent":0,"parameters":["怎么样，是不是很容易就记住了？"]},{"code":401,"indent":0,"parameters":["梨梨，跟我一起唱：“大王具足虫~~~”"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["停！快停下来啊！快给我停下来！善子——！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["嗯？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["虽然我不反对它很有感染力这一点，但要是LOVE LIVE的参赛曲变成"]},{"code":401,"indent":0,"parameters":["这样子的话，绝对会完蛋的吧——！？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["诶——"]},{"code":401,"indent":0,"parameters":["梨梨不喜欢这首歌吗？明明这么就能记住的，还会让人有想要一起"]},{"code":401,"indent":0,"parameters":["唱的冲动……难道学园偶像的歌曲不该是这样子的吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["能记住和让人想要一起唱确实是优点，但是……"]},{"code":401,"indent":0,"parameters":["……"]},{"code":401,"indent":0,"parameters":["这也太过头了吧……"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["那么，对你的作曲有没有什么启发呢？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["拜它所赐，我完全已经把之前储备在脑子里的一些旋律忘得一干二"]},{"code":401,"indent":0,"parameters":["净了，只剩下“大王具足虫~~~”这一段了。"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["也就是说，完全摆脱了固有记忆的束缚，可以用全新的思路来创作"]},{"code":401,"indent":0,"parameters":["了，对吧？嘿嘿嘿，看来堕天使的帮助真是让你醍醐灌顶呢！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["醍醐灌顶……个鬼啊！"]},{"code":401,"indent":0,"parameters":["——也没有堕天使！只是个完完全全的笨蛋吧！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["我现在怀疑那首《具足虫之歌》是不是你路过什么邪教组织时听到"]},{"code":401,"indent":0,"parameters":["的献祭仪式的祝曲……"]},{"code":401,"indent":0,"parameters":["现在报警还来得及吗……啊，不对，或许应该说幸好你没出什么事？"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["真的是我在天界的神社听到的啊！"]},{"code":401,"indent":0,"parameters":["还有，我是堕天使这件事情，千万不能告诉其他人哦，不然的话，"]},{"code":401,"indent":0,"parameters":["我会被审判天使抓走的！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["放心吧，就算在全校师生面前宣布，也不会有人相信的。（包括我）"]},{"code":101,"indent":0,"parameters":["",0,0,2,"善子"]},{"code":401,"indent":0,"parameters":["好，那么现在我们回家吧！学校里只剩下我们两个人了哦！"]},{"code":401,"indent":0,"parameters":["一边唱《具足虫之歌》一边回去吧！"]},{"code":101,"indent":0,"parameters":["",0,0,2,"梨子"]},{"code":401,"indent":0,"parameters":["？"]},{"code":401,"indent":0,"parameters":["不许唱啊！！！"]},{"code":201,"indent":0,"parameters":[0,2,8,7,2,0]},{"code":205,"indent":0,"parameters":[-1,{"list":[{"code":40,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":40,"indent":null}]},{"code":123,"indent":0,"parameters":["C",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"C","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":